/**
 * slicing.du
 *
 * Testing list slicing
 */

var x = [1, 2, 3, 4, 5];

assert(x[0:len(x)] == x);
assert(x[0:] == x);
assert(x[1:] == [2, 3, 4, 5]);
assert(x[2:] == [3, 4, 5]);
assert(x[3:] == [4, 5]);
assert(x[4:] == [5]);
assert(x[5:] == []);

assert(x[:len(x)] == x);
assert(x[:5] == x);
assert(x[:4] == [1, 2, 3, 4]);
assert(x[:3] == [1, 2, 3]);
assert(x[:2] == [1, 2]);
assert(x[:1] == [1]);
assert(x[:0] == []);

assert(x[1:4] == [2, 3, 4]);
assert(x[1:3] == [2, 3]);
assert(x[1:2] == [2]);
assert(x[1:1] == []);

assert(x[2:5] == [3, 4, 5]);
assert(x[2:4] == [3, 4]);
assert(x[2:3] == [3]);
