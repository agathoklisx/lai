/**
* push.du
*
* Testing the list.push() method
*
* .push() will insert a new value at the end of a list
*/

var x = [1, 2, 3, 4, 5];

// First check the list was created properly
assert(x == [1, 2, 3, 4, 5]);

// Test .push()
x.push(6);
assert(x == [1, 2, 3, 4, 5, 6]);
x.push(7);
assert(x == [1, 2, 3, 4, 5, 6, 7]);
x.push(8);
assert(x == [1, 2, 3, 4, 5, 6, 7, 8]);