/**
* pop.du
*
* Testing the list.pop() method
*
* .pop() will remove an element from a list and return the value
*/

var x = [1, 2, 3, 4, 5];

// First check the list was created properly
assert(x == [1, 2, 3, 4, 5]);

// Test .pop()
var y = x.pop();
assert(y == 5);
assert(x == [1, 2, 3, 4]);

// Test .pop(index)
y = x.pop(0);
assert(y == 1);
assert(x == [2, 3, 4]);